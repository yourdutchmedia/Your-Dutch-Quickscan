    "laravel-mix": "^4.1.4",
    "normalize.scss": "^0.1.0",
    "sass": "^1.24.4",
    "slick-carousel": "^1.8.1"
  },
  "browserslist": [
    "last 2 version",
    "> 1%",
    "IE 10"
  ],
  "devDependencies": {
    "browser-sync": "^2.26.10",
    "browser-sync-webpack-plugin": "^2.2.2",
    "cross-env": "^6.0.3",
    "resolve-url-loader": "^2.3.2",
    "sass-loader": "^7.3.1",
    "vue-template-compiler": "^2.6.11"
